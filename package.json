{
  "name": "@master-chief/pretty-table",
  "version": "1.2.0",
  "description": "Format data to a console friendly table.",
  "author": "117",
  "contributors": [
    "AqilCont"
  ],
  "license": "ISC",
  "type": "module",
  "types": "./@types/index.d.ts",
  "main": "./dist/cjs/index.cjs",
  "exports": {
    "import": "./dist/mjs/index.mjs",
    "require": "./dist/cjs/index.cjs"
  },
  "scripts": {
    "build": "npm i && npm run clean && npm run build:mjs && npm run build:cjs && ./remap.sh",
    "build:mjs": "tsc --outDir dist/mjs --moduleResolution node --allowSyntheticDefaultImports -d --declarationDir @types --module esnext --target esnext src/*.ts",
    "build:cjs": "tsc --outDir dist/cjs --moduleResolution node --esModuleInterop src/*.ts",
    "clean": "rimraf dist @types",
    "test": "npm run test:default",
    "test:default": "jest",
    "test:watch": "jest --watchAll"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/*.test.ts"
    ]
  },
  "devDependencies": {
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
}
